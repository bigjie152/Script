# AI provider routing: mock | qwen | deepseek | other
AI_PROVIDER=mock
AI_PROVIDER_DERIVE=qwen
AI_PROVIDER_CHECK=deepseek

# Qwen (generation)
AI_QWEN_API_KEY=
AI_QWEN_MODEL=qwen3-max-2026-01-23
AI_QWEN_BASE_URL=

# DeepSeek (logic check)
AI_DEEPSEEK_API_KEY=
AI_DEEPSEEK_MODEL=DeepSeek-R1
AI_DEEPSEEK_BASE_URL=

# Fallback (optional)
AI_API_KEY=
AI_MODEL=
AI_BASE_URL=

# Cloudflare D1 (local sqlite file)
D1_DB_PATH=./.d1/local.sqlite

# Cloudflare D1 (runtime binding name, optional)
D1_BINDING=DB

# Frontend API base URL (optional). If empty, frontend uses same-origin /api.
NEXT_PUBLIC_API_BASE_URL=
